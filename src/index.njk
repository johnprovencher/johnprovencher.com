<div id="slide-container">
    <div id="counter">
    </div>
    <div id="slideshow">
        {%- for post in collections.post -%}
        {% if post.data.video %}
        <div class="slide" style="left:-5000px">
            <video type="video/mp4" controls='false' preload="none" muted autoplay loop playsinline>
                <source src="{{ post.data.video }}" type="video/mp4">
                <track label="{{post.data.imageAlt}}" label="English" default>
                Your browser does not support the video tag.
            </video>
        </div>
        {% endif %}
        {% if post.data.image %}
        <div class="slide">
            <img class="lozad" data-src="{{post.data.image}}" alt="{{post.data.imageAlt}}">
        </div>
        {% endif %}
        {%- endfor -%}
    </div>
