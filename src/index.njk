    <div id="thumbnails">
        {%- for post in collections.post -%}

        {% if post.data.video %}
        <div class="image-container">
            <video class="lozad" controls='false' muted autoplay="false" loop playsinline>
                    <source data-src="{{ post.data.video }}" type="video/mp4">
                <track label="{{post.data.imageAlt}}" label="English" default>
                Your browser does not support the video tag.
            </video>
                <br>
                <p class="small">{{ post.data.info }}</p>
        </div>
        {% endif %}
        {% if post.data.image %}
        <div class="image-container">
            <img class="lozad" data-src="{{post.data.image}}" alt="{{post.data.imageAlt}}">
                <br>
                <p class="small">{{ post.data.info }}</p>
        </div>
        {% endif %}
        {%- endfor -%}
    </div>
